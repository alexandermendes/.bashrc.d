# Navigation
alias cd..='cd ../'
alias ..='cd ../'
alias .2='cd ../../'
alias .3='cd ../../../'
alias .4='cd ../../../../'
alias .5='cd ../../../../../'

# List
alias ls='ls -lah'
alias l='ls -lah'

# List hidden
alias lsh='ls -ld .?*'

# Clear
alias c='clear'

# Snapshot
alias snap='screencapture -w -c'
